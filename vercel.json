{
  "version": 2,
  "framework": null,
  "buildCommand": "cd voting-tool && npm install",
  "outputDirectory": "voting-tool/public",
  "builds": [
    {
      "src": "voting-tool/api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 10,
        "includeFiles": ["voting-tool/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/voting-tool/api/index.js"
    }
  ],
  "functions": {
    "voting-tool/api/index.js": {
      "runtime": "nodejs18.x"
    }
  }
}
